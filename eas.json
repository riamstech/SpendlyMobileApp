{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "GOOGLE_SERVICE_INFO_PLIST": "$GOOGLE_SERVICE_INFO_PLIST",
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON"
      }
    },
    "preview": {
      "developmentClient": false,
      "distribution": "store",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_SERVICE_INFO_PLIST": "$GOOGLE_SERVICE_INFO_PLIST",
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON"
      }
    },
    "production": {
      "developmentClient": false,
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "GOOGLE_SERVICE_INFO_PLIST": "$GOOGLE_SERVICE_INFO_PLIST",
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON"
      }
    }
  },
  "submit": {
    "production": {},
    "preview": {}
  }
}

